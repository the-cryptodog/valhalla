<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.MainActivity">

    <ImageButton
        android:id="@+id/img_totalBackground"
        android:layout_width="405dp"
        android:layout_height="497dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_gallery" />

    <ImageButton
        android:id="@+id/img_secondBackground"
        android:layout_width="332dp"
        android:layout_height="352dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_totalBackground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/img_totalBackground"
        app:srcCompat="@android:drawable/ic_menu_gallery" />

    <ImageButton
        android:id="@+id/img_joss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="116dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <ImageButton
        android:id="@+id/img_incense_burner"
        android:layout_width="103dp"
        android:layout_height="92dp"
        app:srcCompat="@android:drawable/ic_menu_gallery"
        tools:layout_editor_absoluteX="154dp"
        tools:layout_editor_absoluteY="319dp" />

    <ImageButton
        android:id="@+id/img_flower_left"
        android:layout_width="52dp"
        android:layout_height="52dp"
        app:srcCompat="@android:drawable/ic_menu_gallery"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="201dp" />

    <ImageButton
        android:id="@+id/img_flower_right"
        android:layout_width="52dp"
        android:layout_height="52dp"
        app:srcCompat="@android:drawable/ic_menu_gallery"
        tools:layout_editor_absoluteX="320dp"
        tools:layout_editor_absoluteY="201dp" />

    <ImageButton
        android:id="@+id/img_incense"
        android:layout_width="52dp"
        android:layout_height="52dp"
        app:srcCompat="@android:drawable/ic_menu_gallery"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="275dp" />

    <ImageButton
        android:id="@+id/img_candle_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/title_bar_tall"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="283dp" />

    <ImageButton
        android:id="@+id/img_vase_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/title_bar_tall"
        tools:layout_editor_absoluteX="331dp"
        tools:layout_editor_absoluteY="261dp" />

    <ImageButton
        android:id="@+id/img_vase_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/title_bar_tall"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="261dp" />

    <ImageButton
        android:id="@+id/img_candle_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/title_bar_tall"
        tools:layout_editor_absoluteX="272dp"
        tools:layout_editor_absoluteY="283dp" />

</androidx.constraintlayout.widget.ConstraintLayout>